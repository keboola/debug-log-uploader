{
    "name": "keboola/debug-log-uploader",
    "license": "MIT",
    "type": "library",
    "description": "Keboola Debug Log Uploader",
    "keywords": [
        "keboola",
        "debug",
        "log",
        "uploader",
        "file",
        "s3",
        "abs"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/keboola/zf1-json"
        }
    ],
    "require": {
        "php": ">=8.1",
        "aws/aws-sdk-php": "~3.0",
        "symfony/filesystem": "^4.0|^5.0|^6.0",
        "microsoft/azure-storage-blob": "~1.5"
    },
    "autoload": {
        "psr-4": {
            "Keboola\\": "src/Keboola/"
        }
    },
    "require-dev": {
        "keboola/coding-standard": "^14",
        "phpunit/phpunit": "^9"
    },
    "scripts": {
        "phpcs": "phpcs --standard=psr2 -n --ignore=vendor --extensions=php .",
        "phpunit": "phpunit",
        "ci": [
            "@phpcs",
            "@phpunit"
        ]
    },
    "config": {
        "sort-packages": true,
        "lock": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
